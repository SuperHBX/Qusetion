@model IEnumerable<QADAL.Models.Repair>
@{
    ViewBag.Title = "RepairLog";
    Layout = "~/Views/Shared/_LayoutRepair.cshtml";
}

<h2>RepairLog</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Regman)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Regtime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Manager)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RepairState)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RepairType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RepairNote)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CheckMan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CheckTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CheckNote)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CheckSate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Regman)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Regtime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Manager)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RepairState)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RepairType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RepairNote)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckMan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckNote)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckSate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
