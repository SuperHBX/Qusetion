
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>网上保修与维护</title>
    <link href="~/css/default.css" rel="stylesheet" />
    <link href="~/css/prettify.css" rel="stylesheet" />
    <script src="~/js/kindeditor-min.js"></script>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/zh_CN.js"></script>
    <script src="~/js/jquery.form.js"></script>
    <script src="~/js/prettify.js"></script>-
    <script src="~/js/Common.js"></script>
    <script src="~/js/showCover.js"></script>
    <link href="~/css/showCover.css" rel="stylesheet" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/js/bootstrap.min.js"></script>
    <style type="text/css">
         #c {
             margin: 0 auto;
             width: 96%;
         }
          * {
             list-style: none;
             padding: 0px;
             margin: 0px;
             font-size: 14px;
             font-family: 微软雅黑;
         }
        #header {
        width:100%;
        font-size:30px;   
        color:black;
        background-color:#DBE9FE;
        height:120px;
        line-height:120px;
        vertical-align:middle;
        text-align:center;
        border:1px solid black;
        }
        #tab li{
            color:black;
            background-color:#DBE9FE;
            cursor:pointer;
            float:left;
            height:40px;
            line-height:40px;
            width:17%;
            vertical-align:middle;
            text-align:center;
            border-bottom:black 1px solid;
            border-left:black 1px solid;

        }
        #tab li:last-child {
            border-right:black 1px solid;
            
        }
        .clear {
            clear:both;
        }
        
    </style>
</head>
<body>
    <div id="c">
        <div id="header">
            网上保修与维护 
        </div>
        <div >
            <ul id="tab" style="margin:0px;">
                <li>保修申请</li>
                <li>保修维护管理</li>
                <li>保修记录</li>
                <li>基地维护</li>
            </ul>
            <div class="clear"></div>
        </div>
        <div id="body">
            @RenderBody()
        </div>
    </div>
</body>
</html>
